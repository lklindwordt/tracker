define "helpers", [], ->
  class Helper
    searchToParam: (str) ->
      obj = {}
      parts = str.split '&'
      for part in parts
        kv = part.split '='
        [key, value] = kv
        obj[key] = value

      obj

    scriptQuery: (identifier) ->
      scripts = document.getElementsByTagName('script')
      for script in scripts
        if script.src.match identifier
          return @searchToParam (script.src.match(/\?.*/) || [undefined])[0];
      ""

    reverseMerge: (other, source) ->
      for key of source
        if !(key of other)
          other[key] = source[key]
      other

  new Helper()